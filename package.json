{
  "name": "manga",
  "version": "1.0.0",
  "description": "microservice for manga",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node -r tsconfig-paths/register build/index.js",
    "dev": "nodemon",
    "build": "rm -rf ./build && npx tsc",
    "doc:html": "rm -rf ./docs && npx typedoc --tsconfig ./tsconfig.json",
    "doc:md": "rm -rf ./docs && npx typedoc --plugin typedoc-plugin-markdown --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "manga",
    "manga-eden",
    "open-source"
  ],
  "author": "Tushar Singh",
  "license": "ISC",
  "dependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.24",
    "@types/node": "^14.0.13",
    "@types/node-cron": "^2.0.3",
    "@types/string-similarity": "^3.0.0",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.19",
    "node-cron": "^2.0.3",
    "nodemon": "^2.0.4",
    "string-similarity": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-typedoc": "^2.2.5",
    "typedoc": "^0.18.0",
    "typedoc-plugin-markdown": "^2.4.2"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
}
